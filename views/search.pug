extends layouts/main
include mixins/videoBlock
block content
    .search_header
        h3 Searching for #{searchingBy}
    .search_videos
        if videos.length === 0
            h5 No Videos Found
        each item in videos
            +videoBlock({
                fileUrl : item.fileUrl,
                title : item.title,
                views : item.views,
                description : item.description,
                id : item.id
            })